{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "rules": {
    "no-console": 0,
    "prefer-destructuring": [
      0
    ]
  },
  "env": {
    "es6": true,
    "node": true
  },
  "overrides": [
    {
      "files": [
        "src/test/**/*.js"
      ],
      "env": {
        "mocha": true
      },
      "rules": {
        "import/no-extraneous-dependencies": 0
      }
    },
    {
      "files": [
        "src/**/*.test.js",
        "src/utils/test.js"
      ],
      "env": {
        "jest": true
      }
    }
  ]
}